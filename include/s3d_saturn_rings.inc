#macro SaturnRings()
  #declare i=1;
  #declare Width=140460/74500-1; // The width of the ring system, inner radius being 1 unit.
  #declare Step=Width/15;       // Due to the 8-bit entry-limitation in colour_map, the 
  
  #declare saturn_rings = union {
  #while (i<=15)                // ring system is divided into 15 discs.
    disc {                        
      <0, 0, 0> y,
      1+i*Step,       // outer radius
      1+(i-1)*Step    // inner radius
      pigment {
        onion
        colour_map { Map(i) }
      }
      finish {ambient 0 diffuse 1}  // try increasing diffuse if the rings are too dark
      scale 74500
      hollow on
    }
    #declare i=i+1;
  #end
  hollow on
  }
  
  object { saturn_rings hollow on }

#end //macro
